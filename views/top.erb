<h1><%= @title_h1 %></h1>
<ul>
  <% @articles.each do |article| %>
    <li data-id="<%= article.id %>" data-token="<%= csrf_token %>">
      <a href="/bulletinboard/article/<%= article.id %>">
        <%= h(article.title) %>
      </a>
      <% if article.user_id == session[:user_id] %>
        <span class="article_delete">[公開終了]</span>
      <% end %>
    </li>
  <% end %>
</ul>
